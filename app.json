{
  "expo": {
    "name": "mobaccess2",
    "slug": "mobaccess2",
    "scheme": "mobacces2",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/spa7.jpeg",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    }
  }
}
